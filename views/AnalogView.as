package views {		import views.AbstractClock;	import models.ClockModel;			public class AnalogView extends AbstractClock {						public function AnalogView(clockModel:ClockModel) {			super(clockModel);		}				override protected function updateTime():void {			SecHand.rotation = 6 * _model.seconds;			MinHand.rotation = 6 * _model.minutes;			HourHand.rotation = 30 * _model.hours + _model.minutes / 2;		}	}	}