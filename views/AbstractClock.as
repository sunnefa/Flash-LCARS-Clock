package views {		import flash.display.Sprite;	import models.ClockModel;	import flash.events.Event;		public class AbstractClock extends Sprite {				protected var _model:ClockModel;				public function AbstractClock(timeModel:ClockModel) {			_model = timeModel;			_model.addEventListener(Event.CHANGE, onTimeChange);		}				private function onTimeChange(e:Event):void {			updateTime();		}				protected function updateTime():void {					}	}	}