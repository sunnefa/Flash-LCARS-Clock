package controllers {		import models.ClockModel;		public class ClockController {				private var _model:ClockModel;		public function ClockController(theModel:ClockModel) {			_model = theModel;		}				public function changeHours(val):void {			if(val <= 0) {				val = 23;				changeDay(_model.day - 1);			}			_model.hours = val;		}				public function changeMins(val):void {			if(val <= 0) {				val = 59;				changeHours(_model.hours - 1);			}			_model.minutes = val;		}				public function changeSecs(val):void {			if(val <= 0) {				val = 59;				changeMins(_model.minutes - 1);			}			_model.seconds = val;		}				public function changeDay(val):void {			_model.day = val;		}				public function changeMonth(val):void {			if(val < 0) {				val = 11;				changeYear(_model.year - 1);			}			_model.month = val;		}				public function changeYear(val):void {			_model.year = val;		}	}	}